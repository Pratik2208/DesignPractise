<?xml version="1.0" encoding="utf-8" ?>
<ContentPage
    x:Class="DesignPractise.Pages.MainPage"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:model="clr-namespace:DesignPractise.Models"
    xmlns:viewmodel="clr-namespace:DesignPractise.ViewModel"
    x:DataType="viewmodel:UserViewModel"
    BackgroundColor="Transparent"
    Shell.NavBarIsVisible="False">

    <Grid RowDefinitions="Auto,Auto,Auto,Auto,Auto,Auto">
        <Label
            Grid.Row="0"
            Margin="0,20"
            FontSize="Title"
            HorizontalOptions="Center"
            Text="{Binding Username}"
            TextColor="SkyBlue"
            VerticalOptions="Center" />

        <Border
            Grid.Row="1"
            Grid.Column="0"
            Margin="30,0,0,0"
            HorizontalOptions="Start"
            StrokeShape="Rectangle 25"
            WidthRequest="150">

            <Frame>
                <VerticalStackLayout>
                    <Image
                        Margin="0,10,0,0"
                        HeightRequest="80"
                        Source="doctor.png">
                        <Image.GestureRecognizers>
                            <TapGestureRecognizer Tapped="TapGestureRecognizer_Tapped_1" />
                        </Image.GestureRecognizers>
                    </Image>
                    <Label
                        Margin="0,10"
                        FontAttributes="Bold"
                        HorizontalOptions="Center"
                        Text="Doctor" />
                    <Image
                        x:Name="img2"
                        HeightRequest="30"
                        IsVisible="false"
                        Source="data:image/png;base64," />
                </VerticalStackLayout>
            </Frame>
        </Border>

        <Border
            Grid.Row="1"
            Grid.Column="1"
            Margin="0,0,30,0"
            HorizontalOptions="End"
            StrokeShape="Rectangle 25"
            WidthRequest="150">
            <Frame>
                <VerticalStackLayout>
                    <Image
                        Margin="0,10,0,0"
                        HeightRequest="80"
                        Source="profile.png">
                        <Image.GestureRecognizers>
                            <TapGestureRecognizer Tapped="TapGestureRecognizer_Tapped" />
                        </Image.GestureRecognizers>
                    </Image>

                    <Label
                        Margin="0,10"
                        FontAttributes="Bold"
                        HorizontalOptions="Center"
                        Text="Patient" />
                    <Image
                        x:Name="img"
                        HeightRequest="30"
                        IsVisible="false"
                        Source="data:image/png;base64," />
                </VerticalStackLayout>
            </Frame>
        </Border>


        <StackLayout Grid.Row="2" Orientation="Vertical">
            <Border
                x:Name="bemail"
                Margin="10,40,10,0"
                StrokeShape="RoundRectangle 15"
                StrokeThickness="3">
                <Grid ColumnDefinitions="*,*,*,*,*" RowDefinitions="*">
                    <Image
                        Grid.Column="0"
                        HeightRequest="35"
                        Source="data:image/png;base64,"
                        VerticalOptions="Center" />
                    <Entry
                        Grid.Column="1"
                        Grid.ColumnSpan="3"
                        BackgroundColor="Green"
                        Focused="Entry_Focused"
                        HorizontalOptions="Start"
                        MaximumWidthRequest="150"
                        Placeholder="Email"
                        Unfocused="Entry_Unfocused"
                        VerticalOptions="Center" />

                </Grid>
            </Border>
        </StackLayout>

        <StackLayout Grid.Row="3" Orientation="Vertical">
            <Border
                x:Name="bpassword"
                Margin="10,25,10,0"
                StrokeShape="RoundRectangle 15"
                StrokeThickness="3">
                <!--
                <HorizontalStackLayout>
                    <Image
                        HeightRequest="20"
                        Source="data:image/png;base64,"
                        VerticalOptions="Center"
                        WidthRequest="50" />
                    <Entry
                        HorizontalOptions="Center"
                        MaximumWidthRequest="90"
                        Placeholder="Password"
                        VerticalOptions="Center" />
                    <Button
                        Margin="120,0,0,0"
                        BackgroundColor="Transparent"
                        Text="Forgot"
                        TextColor="blue" />
                </HorizontalStackLayout>
                -->

                <Grid ColumnDefinitions="*,*,*,*,*" RowDefinitions="*">
                    <Image
                        Grid.Column="0"
                        HeightRequest="20"
                        Source="data:image/png;base64,"
                        VerticalOptions="Center" />
                    <Entry
                        Grid.Column="1"
                        Grid.ColumnSpan="3"
                        BackgroundColor="Green"
                        Focused="Entry_Focused_1"
                        HorizontalOptions="Start"
                        MaximumWidthRequest="150"
                        Placeholder="Password"
                        Unfocused="Entry_Unfocused_1"
                        VerticalOptions="Center" />
                    <Button
                        Grid.Column="4"
                        BackgroundColor="Transparent"
                        Text="Forgot"
                        TextColor="blue" />
                </Grid>
            </Border>
            <Button Command="{Binding LoginCommand}" Text="login" />
        </StackLayout>


    </Grid>

</ContentPage>
