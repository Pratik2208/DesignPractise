<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="DesignPractise.Pages.MainPage"
             BackgroundColor="Transparent"
             x:Name="page"
             Shell.NavBarIsVisible="False"
            >

    <Grid RowDefinitions="Auto,Auto,Auto,Auto,Auto,Auto">
        <Label 
            Margin="0,20"
            Grid.Row="0" 
            Text="Choose Account Type"
            TextColor="SkyBlue" 
            FontSize="Title" 
            HorizontalOptions="Center" 
            VerticalOptions="Center" />

        <Border 
            HorizontalOptions="Start"
            Margin="30,0,0,0"
            WidthRequest="150"
            StrokeShape="Rectangle 25" 
            Grid.Row="1"
            Grid.Column="0">

            <Frame>
                <VerticalStackLayout>
                    <Image Margin="0,10,0,0" Source="doctor.png" HeightRequest="80">
                        <Image.GestureRecognizers>
                            <TapGestureRecognizer Tapped="TapGestureRecognizer_Tapped_1"/>
                        </Image.GestureRecognizers>
                    </Image>
                   <Label 
                     FontAttributes="Bold"
                     Text="Doctor" 
                     HorizontalOptions="Center" 
                     Margin="0,10" />
                    <Image
                       IsVisible="false"
                       x:Name="img2"
                       HeightRequest="30" 
                       Source="data:image/png;base64," />
                </VerticalStackLayout>
            </Frame>
         </Border>

        <Border 
             Margin="0,0,30,0"
             HorizontalOptions="End"
             WidthRequest="150"
             StrokeShape="Rectangle 25" 
             Grid.Row="1"
             Grid.Column="1">
            <Frame>
                <VerticalStackLayout>
                    <Image Margin="0,10,0,0" Source="profile.png" HeightRequest="80">
                        <Image.GestureRecognizers>
                            <TapGestureRecognizer Tapped="TapGestureRecognizer_Tapped"/>
                        </Image.GestureRecognizers>
                    </Image>

                    <Label 
                       FontAttributes="Bold"
                       Text="Patient" 
                       HorizontalOptions="Center" 
                       Margin="0,10" />
                    <Image
                       IsVisible="false"
                       x:Name="img"
                       HeightRequest="30" 
                       Source=
                        "data:image/png;base64," />
                </VerticalStackLayout>
            </Frame>
        </Border>

    </Grid>

</ContentPage>
